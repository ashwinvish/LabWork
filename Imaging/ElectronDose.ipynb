   "source": [
    "# to calculate electron dose on the sample\n",
    "Const = 1/1.602107657e-19;\n",
    "beamCurrent  = 3e-9 ; # in ampers\n",
    "dwellTime = 200e-9 ; # in seconds\n",
    "pixelSize = 25 ; #nm^2\n",
    "\n",
    "electronDose = (beamCurrent*Const*dwellTime)/pixelSize; # in e/nm^2\n",
    "display(electronDose)"
   ]
